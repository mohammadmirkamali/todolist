    "next-redux-wrapper": "^7.0.5",
    "plyr-react": "^4.0.0",
    "preact": "^10.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.15.1",
    "react-indiana-drag-scroll": "^2.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
